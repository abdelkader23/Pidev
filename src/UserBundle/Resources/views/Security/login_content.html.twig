{% block register %}
    <div id="listar-loginsingup" class="listar-loginsingup">

        <a class="listar-btnclose" href="{{ path('user_layoutUser') }}">x</a>
        <figure class="listar-loginsingupimg" data-vide-bg="poster: {{ asset('UserBundle/images/divert.jpg') }}"
                data-vide-options="position: 50% 50%"></figure>
        <div class="listar-contentarea">
            <div class="listar-themescrollbar">
                <div class="listar-logincontent">
                    <div class="listar-themetabs">
                        <ul class="listar-tabnavloginregistered" role="tablist">
                            <li role="presentation" class="active"><a href="#listar-loging" data-toggle="tab">Log in</a>
                            </li>
                            <li role="presentation"><a href="{{ path('fos_user_registration_register') }}">Register</a>
                            </li>
                        </ul>
                        <div class="tab-content listar-tabcontentloginregistered">
                            <div role="tabpanel" class="tab-pane active fade in" id="listar-loging">
                                <form class="listar-formtheme listar-formlogin"
                                      action="{{ path("fos_user_security_check") }}" method="post">
                                    <fieldset>
                                        {% if csrf_token %}
                                            <input type="hidden" name="_csrf_token" value="{{ csrf_token }}"/>
                                        {% endif %}
                                        <div class="form-group listar-inputwithicon">
                                            <i class="icon-profile-male"></i>
                                            <input class="form-control"
                                                   placeholder="Username Or Email" type="text" id="username"
                                                   name="_username" value="{{ last_username }}" required="required">
                                        </div>
                                        <div class="form-group listar-inputwithicon">
                                            <i class="icon-icons208"></i>
                                            <input class="form-control"
                                                   placeholder="Password" type="password" id="password" name="_password"
                                                   required="required">
                                        </div>
                                        <div class="form-group">
                                            <div class="listar-checkbox">
                                                <input type="checkbox" id="remember_me" name="_remember_me" value="on">
                                                <label for="rememberpass2">Remember me</label>
                                                {% if error %}
                                                    <div class="alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                                                {% endif %}
                                            </div>
                                            <span><a href="{{ path('fos_user_resetting_request') }}">Lost your Password?</a></span>
                                        </div>
                                        <input class="listar-btn listar-btngreen" type="submit" id="_submit"
                                               name="_submit" value="login"/>

                                    </fieldset>
                                </form>
                            </div>

                        </div>
                    </div>
                    <div class="listar-shareor"><span>or</span></div>
                    <div class="listar-signupwith">
                        <h2>Sign in With...</h2>
                        <ul class="listar-signinloginwithsocialaccount">
                            <li class="listar-facebook"><a href="javascript:void(0);"><i
                                            class="icon-facebook-1"></i><span>Facebook</span></a></li>
                            <li class="listar-twitter"><a href="javascript:void(0);"><i
                                            class="icon-twitter-1"></i><span>Twitter</span></a></li>
                            <li class="listar-googleplus"><a href="javascript:void(0);"><i
                                            class="icon-google4"></i><span>Google +</span></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}

