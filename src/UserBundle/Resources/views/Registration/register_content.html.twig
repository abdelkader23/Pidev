       </head>
   {% endblock %}



{% block register %}
    <div id="listar-loginsingup" class="listar-loginsingup">

        <a class="listar-btnclose" href="{{ path('user_layoutUser') }}">x</a>
        <figure class="listar-loginsingupimg" data-vide-bg="poster: {{ asset('UserBundle/images/divert.jpg') }}"
                data-vide-options="position: 50% 50%"></figure>
        <div class="listar-contentarea">
            <div class="listar-themescrollbar">
                <div class="listar-logincontent">
                    <div class="listar-themetabs">
                        <ul class="listar-tabnavloginregistered" role="tablist">
                            <li role="presentation"><a href="{{ path('fos_user_security_login') }}">Log in</a>
                            </li>
                            <li role="presentation" class="active"><a
                                        href="{{ path('fos_user_registration_register') }}">Register</a></li>
                        </ul>
                        <!-- Register form -->
                        <div role="tabpanel" id="listar-register">
                            <form class="listar-formtheme listar-formlogin">
                                <fieldset class="form-group row">

                                    <div class="form-group listar-inputwithicon">
                                        <i class="icon-profile-male"> </i>
                                        {{ form_widget(form.username, {'attr': {'class': 'form-control','placeholder':'Username'}}) }}
                                    </div>
                                    <div class="form-group listar-inputwithicon">
                                        <i class="icon-icons208"></i>
                                        {{ form_widget(form.email, {'attr': {'class': 'form-control','placeholder':'Email Address'}}) }}

                                    </div>

                                    <div class="form-group listar-inputwithicon">
                                        <i class="icon-icons208"></i>
                                        {{ form_widget(form.phone, {'attr': {'class': 'form-control','placeholder':'phone'}}) }}

                                    </div>
                                    <div class="form-group listar-inputwithicon">
                                        <i class="icon-lock-stripes"></i>

                                        {{ form_widget(form.plainPassword.first, {'attr': {'class': 'form-control','placeholder':'Password'}}) }}

                                    </div>
                                    <div class="form-group listar-inputwithicon">
                                        <i class="icon-lock-stripes"></i>
                                        {{ form_widget(form.plainPassword.second, {'attr': {'class': 'form-control','placeholder':'Password confirmation'}}) }}

                                    </div>
                                    <div class="form-group listar-inputwithicon">
                                        <i class="icon-profile-male"> </i>
                                        {{ form_widget(form.nom, {'attr': {'class': 'form-control','placeholder':'Nom'}}) }}
                                    </div>
                                    <div class="form-group listar-inputwithicon">
                                        <i class="icon-profile-male"> </i>
                                        {{ form_widget(form.prenom, {'attr': {'class': 'form-control','placeholder':'Prenom'}}) }}
                                    </div>
                                    <div>

                                    </div>
                                    {{ form_end(form) }}


                                    <input type="submit" value="Register" class="listar-btn listar-btngreen"/>

                                </fieldset>
                            </form>
                        </div>

                    </div>
                    <!-- Social Login -->
                    <div class="listar-shareor"><span>or</span></div>
                    <div class="listar-signupwith">
                        <h2>Sign in With...</h2>
                        <ul class="listar-signinloginwithsocialaccount">
                            <li class="listar-facebook"><a href="javascript:void(0);"><i
                                            class="icon-facebook-1"></i><span>Facebook</span></a></li>
                            <li class="listar-twitter"><a href="javascript:void(0);"><i
                                            class="icon-twitter-1"></i><span>Twitter</span></a></li>
                            <li class="listar-googleplus"><a href="javascript:void(0);"><i
                                            class="icon-google4"></i><span>Google +</span></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}




{{ form_end(form) }}
